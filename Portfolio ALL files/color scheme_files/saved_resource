<script type="d6b594575347813a1efa2822-text/javascript">

    $(document).ready(function(){
      if(!checkCookie("skillshare","1") && 1 == 2){
        setTimeout(function(){
		  if($(window).width()>600)
          stickyToggle($("#skillshare-sticky"), true);
        }, 2000);
      }
    });

    $("#skillshare-sticky-btn").on("click", function(){
      stickyToggle($("#skillshare-sticky"), false);
      showModal($modal, $("#modal-skillshare"), "skillshare", function(){});
      return false;
    });

    var skillshare_classes_item = 0;

    function skillshareClassesScroll(){
      skillshareClassesCheckBtns();
      var scroll = skillshare_classes_item*(227+15);
      $("#skillshare-classes .scroller").css("transform", "translate3d(-"+scroll+"px,0,0)")
    }

    function skillshareClassesCheckBtns(){
      $("#skillshare-prev-btn").toggleClass("disabled", skillshare_classes_item == 0);
      $("#skillshare-next-btn").toggleClass("disabled", skillshare_classes_item>=$("#skillshare-classes .item").length-3);
    }

    $("#skillshare-next-btn").on("click", function(){
      if($(this).is(".disabled")) return false;
      skillshare_classes_item++;
      skillshareClassesScroll();
      return false;
    });

    $("#skillshare-prev-btn").on("click", function(){
      if($(this).is(".disabled")) return false;
      skillshare_classes_item--;
      skillshareClassesScroll();
      return false;
    });

    skillshareClassesCheckBtns();

    $("#skillshare-modal-close-btn").on("click", function(){
      hideModal();
      setCookie("skillshare","1");
      return false;
    });

    $("#skillshare-modal-get-btn, #skillshare-modal .close-btn, #skillshare-sticky .sticky-close-btn").on("click", function(){
      setCookie("skillshare","1");
    });

$(window).on('keydown', function(e){
	if(e.keyCode == 32)
	ga('send', 'event', 'Actions', 'spacebar', '');
});

  </script>

<style id="instagram-style"></style>
<script type="d6b594575347813a1efa2822-text/javascript">

  if($(window).width() > 880 && !checkCookie('instagram', '1')){
    $('#instagram-bar').show();
    $('#instagram-style').html(`
      header { top: 45px; }
      #sub-header { top: 100px; }
      #app-container, #browser-container { height: calc(100% - 155px)!important; top: 155px!important; }
    `);
  }

  $('#instagram-bar-close').click(function(){
    $('#instagram-bar').hide();
    $('#instagram-style').html('');
    setCookie('instagram', '1', 7);
  });


  </script>
